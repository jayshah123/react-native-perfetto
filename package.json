{
  "name": "react-native-perfetto",
  "version": "0.1.0",
  "description": "React Native Perfetto tracing and recording library",
  "main": "./lib/module/index.js",
  "react-native": "./src/index.tsx",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "react-native": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "scripts",
    ".maestro",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-perfetto-example",
    "example:legacy": "yarn workspace react-native-perfetto-example-legacy",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build example-legacy/android/build example-legacy/android/app/build example-legacy/ios/build lib",
    "build": "bob build",
    "prepare": "bob build",
    "vendor:perfetto-sdk": "./scripts/vendor-perfetto-sdk.sh",
    "maestro:install": "./scripts/install-maestro.sh",
    "maestro:test:capture-trace": "MAESTRO_PLATFORM=android ./scripts/run-maestro.sh test .maestro/capture-trace.yaml",
    "maestro:test:capture-trace:legacy": "MAESTRO_PLATFORM=android ./scripts/run-maestro.sh test .maestro/capture-trace-legacy.yaml",
    "maestro:test:capture-busy-loop-1s": "MAESTRO_PLATFORM=android ./scripts/run-maestro.sh test .maestro/capture-busy-loop-1s.yaml",
    "maestro:test:capture-webview-trace": "MAESTRO_PLATFORM=android ./scripts/run-maestro.sh test .maestro/capture-webview-trace.yaml",
    "trace:pull:android": "./scripts/pull-latest-android-trace.sh",
    "trace:pull:ios": "./scripts/pull-latest-ios-trace.sh",
    "playwright:test:verify-trace": "node ./scripts/verify-trace-playwright.mjs",
    "playwright:test:verify-busy-loop-1s": "node ./scripts/verify-trace-playwright.mjs --event busy-loop-1s --min-dur-ms 900 --max-dur-ms 1500",
    "playwright:test:verify-webview-trace": "node ./scripts/verify-trace-playwright.mjs --event webview-demo-section",
    "maestro:test:capture-and-verify-trace": "yarn maestro:test:capture-trace && yarn trace:pull:android && yarn playwright:test:verify-trace -- --headed",
    "maestro:test:capture-and-verify-busy-loop-1s": "yarn maestro:test:capture-busy-loop-1s && yarn trace:pull:android && yarn playwright:test:verify-busy-loop-1s -- --headed",
    "maestro:test:capture-and-verify-webview-trace": "yarn maestro:test:capture-webview-trace && yarn trace:pull:android && yarn playwright:test:verify-webview-trace -- --headed",
    "maestro:test:capture-and-verify-busy-loop-1s:ios": "MAESTRO_PLATFORM=ios yarn maestro:test:capture-busy-loop-1s && yarn trace:pull:ios && yarn playwright:test:verify-busy-loop-1s -- --headed",
    "maestro:test": "yarn maestro:test:capture-trace",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "perfetto",
    "tracing",
    "performance"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayshah/react-native-perfetto.git"
  },
  "author": "Jay Shah <jay.shah@example.com> (https://github.com/jayshah)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jayshah/react-native-perfetto/issues"
  },
  "homepage": "https://github.com/jayshah/react-native-perfetto#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@types/react": "^19.2.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "playwright": "^1.58.2",
    "prettier": "^2.8.8",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-builder-bob": "^0.40.18",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example",
    "example-legacy"
  ],
  "packageManager": "yarn@4.11.0",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "PerfettoSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.perfetto"
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "create-react-native-library": {
    "type": "turbo-module",
    "languages": "kotlin-objc",
    "tools": [
      "eslint"
    ],
    "version": "0.57.1"
  }
}
